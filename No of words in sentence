import java.util.*;
class Main{
    public static void main(String[] args) {
       String str = "Prema is crazy";
       String[] words = str.trim().split(" ");
     
       int lenWord = words.length;
       System.out.println(lenWord);
       
       for (int i =0; i< words.length; i++){
           System.out.println(words[i]);
       }
    }
}
